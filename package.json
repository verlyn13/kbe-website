    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit",
    "test:a11y": "vitest run src/**/__a11y__/*.test.tsx",
    "pw": "playwright test",
    "pw:ci": "playwright test --reporter=list,json",
